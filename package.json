{
  "author": "Olivier Combe",
  "name": "subnode",
  "description": "An app to easily get subtitles for your tv show",
  "version": "1.0.3",
  "engines": {
    "node": ">=0.8"
  },
  "scripts": {
    "start": "node_modules/.bin/brunch watch --server",
    "test": "node_modules/.bin/karma start test/karma.conf.js",
    "postinstall": "coffee --output ./node_modules/tvdb/lib --compile ./node_modules/tvdb/src"
  },
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ocombe/subNode"
  },
  "keywords": [
    "subtitles",
    "tv",
    "show",
    "tv show"
  ],
  "license": "BSD",
  "gitHead": "386272067cd718cb0f4e04d80ad2ae4371b945df",
  "readmeFilename": "README.md",
  "dependencies": {
    "wrench": ">= 1.5.1",
    "async": ">= 0.2.6",
    "request": ">= 2.25.0",
    "natural": ">= 0.1.19",
    "adm-zip": ">= 0.4.3",
    "sickbeard": "0.0.2",
    "nconf": ">= 0.6.7",
    "tvdb": "0.0.12",
    "express": ">= 3.3.1",
    "lodash": ">= 1.3.1",
    "cheerio": ">= 0.12.1",
    "forever-monitor": "git://github.com/abueckerMB/forever-monitor.git",
    "coffee-script": "~1.6.3"
  },
  "devDependencies": {
    "brunch": ">= 1.7.0",
    "auto-reload-brunch": ">= 1.6.4",
    "javascript-brunch": ">= 1.6",
    "css-brunch": ">= 1.6",
    "sass-brunch": ">= 1.6.1",
    "uglify-js-brunch": ">= 1.6",
    "clean-css-brunch": ">= 1.6",
    "bower": ">= 1.0.3"
  }
}
