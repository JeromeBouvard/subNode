{
  "author": "Olivier Combe <olivier.combe@gmail.com> (https://github.com/ocombe)",
  "name": "subnode",
  "description": "An app to easily get subtitles for your tv show",
  "version": "1.0.8",
  "engines": {
    "node": ">=0.8"
  },
  "scripts": {
    "start": "node_modules/.bin/brunch watch --server",
    "test": "node_modules/.bin/karma start test/karma.conf.js",
    "postinstall": "coffee --output ./node_modules/tvdb/lib --compile ./node_modules/tvdb/src"
  },
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ocombe/subNode"
  },
  "keywords": [
    "subtitles",
    "tv",
    "show",
    "tv show"
  ],
  "license": "BSD",
  "gitHead": "386272067cd718cb0f4e04d80ad2ae4371b945df",
  "readmeFilename": "README.md",
  "dependencies": {
    "wrench": ">= 1.5.1",
    "async": ">= 0.2.6",
    "request": "~2.26.0",
    "natural": ">= 0.1.19",
    "adm-zip": ">= 0.4.3",
    "sickbeard": "0.0.2",
    "nconf": ">= 0.6.7",
    "tvdb": ">= 0.0.13",
    "express": "~3.3.5",
    "lodash": ">= 1.3.1",
    "cheerio": ">= 0.12.1",
    "coffee-script": "~1.6.3",
    "forever-monitor": "~1.2.2"
  },
  "devDependencies": {
    "brunch": ">= 1.7.0",
    "auto-reload-brunch": ">= 1.6.4",
    "javascript-brunch": ">= 1.6",
    "css-brunch": ">= 1.6",
    "sass-brunch": ">= 1.6.1",
    "uglify-js-brunch": ">= 1.6",
    "clean-css-brunch": ">= 1.6",
    "bower": "~1.1.2"
  }
}
